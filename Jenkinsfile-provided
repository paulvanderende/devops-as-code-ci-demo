pipeline {

    agent any

    stages {
        stage("Apply XLD") {
            steps {
               echo 'Applying XLD'
               def ret = sh(script: '/tmp/xl apply -f yaml/xld.yaml --xl-deploy-url http://xld:4516/ --xl-deploy-username admin --xl-deploy-password admin', returnStdout: true)
               println ret
            }
        }
    }

}
